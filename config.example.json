{
    "server": {
        "port": 3000, // the port to listen on
        "url": "http://ip:port", // url of this api
        "api": { // enable of disable api implementation
            "subsonic": {
                "enable": true,
                "options": {
                    "zw": true // enable zero width character hack
                }
            },
            "jellyfin": true,
            "euterpe": true
        },
        "users": [ // the subsonic implementation requires a list of SwingMusic users (in the future other implementations will use this)
            {
                "username": "admin",
                "password": "admin"
            }
        ]
    },
    "music": "http://ip:port" // url of the swing music server
}